/* subpackagesA/double-zoom/double-zoom.wxss */
page{background: #eee;}
.content{margin-top: 200rpx;}
.scroll-view{width:100%;height: 220px;background: #C3E56A;}
.ul{padding:20rpx;width: max-content;}
.ul .li .item{border:1px solid #999;background: #eee;flex-shrink: 0;transition:width 1s, height 1s,font-size 1s, margin 1s;}
.ul .li .item.on{background: #DD5044;border-color: #DD5044;color:#fff;}

